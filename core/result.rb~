class Result
  attr_accessor :test_name, :status
  attr_reader   :run_time, :log

  PASS     =  0
  FAIL     = -1
  ERROR    = -2
  ABORT    = -3
  SKIP     = -4
  NOT_RUN  = -5
  RUNNING  = -6

  def initialize(test_name)
    @status = NOT_RUN
    @log = []
    @run_time = Struct.new(:start_time,:end_time,:run_time)
  end

  def start
    

end
